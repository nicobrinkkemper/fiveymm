{
  "name": "fiveymm",
  "version": "1.0.0",
  "private": true,
  "proxy": "https://5ymm.warp.world/",
  "homepage": "https://5ymm.warp.world/",
  "dependencies": {},
  "scripts": {
    "start": "craco start",
    "serve": "serve build",
    "build": "craco build",
    "postbuild": "react-snap",
    "test": "npm-run-all csvtojson test:start",
    "test:start": "craco test",
    "csvtojson": "node ./scripts/csvToJson.js",
    "assets": "npm-run-all logo_with_card logo_without_card mario_illustration",
    "logo_with_card": "convert -resize 272 ./src/assets/original_logo_with_card.png ./src/assets/logo_with_card1x.png; convert -resize 548 ./src/assets/original_logo_with_card.png ./src/assets/logo_with_card2x.png; convert -resize 1096 ./src/assets/original_logo_without_card.png ./src/assets/logo_with_card3x.png",
    "logo_without_card": "convert -resize 272 ./src/assets/original_logo_without_card.png ./src/assets/logo_without_card1x.png; convert -resize 548 ./src/assets/original_logo_without_card.png ./src/assets/logo_without_card2x.png; convert -resize 1096 ./src/assets/original_logo_without_card.png ./src/assets/logo_without_card3x.png",
    "mario_illustration": "convert -resize 140 ./src/assets/original_illustration_mario.png ./src/assets/illustration_mario1x.png; convert -resize 280 ./src/assets/original_illustration_mario.png ./src/assets/illustration_mario2x.png; convert -resize 560 ./src/assets/original_illustration_mario.png ./src/assets/illustration_mario3x.png",
    "images": "npm-run-all build:images move:images",
    "build:images": "cross-env TS_NODE_PROJECT=\"./scripts/tsconfig.json\" webpack --config ./scripts/webpack.config.ts",
    "move:images": "rm ./scripts/makerImages/makerImages.bundle.js; rm ./scripts/levelImages/levelImages.bundle.js; cp -R scripts/levelImages public; cp -R scripts/makerImages public; rm -rf scripts/levelImages; rm -rf scripts/makerImages;"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@fullstory/browser": "^1.4.9",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/node": "^12.20.23",
    "@types/papaparse": "^5.2.6",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "@types/webpack-env": "^1.16.2",
    "clipboard": "^2.0.8",
    "craco-csv-loader": "^1.0.6",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.0.3",
    "fp-ts": "^2.11.1",
    "htmdx": "^0.3.5",
    "jimp": "^0.16.1",
    "lodash": "^4.17.21",
    "mdx.macro": "^0.2.9",
    "memoize-one": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.2",
    "puppeteer": "^10.2.0",
    "react": "^17.0.2",
    "react-accessible-accordion": "^3.3.5",
    "react-app-polyfill": "^2.0.0",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-gtm-module": "^2.0.11",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-snap": "^1.23.0",
    "react-youtube": "^7.13.1",
    "responsive-loader": "^2.3.0",
    "serve": "^12.0.0",
    "sharp": "^0.28.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "web-vitals": "^1.1.2",
    "webpack-cli": "^4.8.0"
  }
}
